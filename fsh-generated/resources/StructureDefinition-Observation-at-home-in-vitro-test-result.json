{
  "resourceType": "StructureDefinition",
  "id": "Observation-at-home-in-vitro-test-result",
  "url": "http://hl7.org/fhir/us/home-lab-report/StructureDefinition/Observation-at-home-in-vitro-test-result",
  "version": "1.0.0",
  "name": "ObservationAtHomeInVitroTestResult",
  "title": "Observation - At-Home In-Vitro Test Result",
  "status": "active",
  "description": "This profile constrains the US Core Laboratory Result Observation to At-Home In-Vitro Sample Tests.",
  "fhirVersion": "4.0.1",
  "mapping": [
    {
      "identity": "workflow",
      "uri": "http://hl7.org/fhir/workflow",
      "name": "Workflow Pattern"
    },
    {
      "identity": "sct-concept",
      "uri": "http://snomed.info/conceptdomain",
      "name": "SNOMED CT Concept Domain Binding"
    },
    {
      "identity": "v2",
      "uri": "http://hl7.org/v2",
      "name": "HL7 v2 Mapping"
    },
    {
      "identity": "rim",
      "uri": "http://hl7.org/v3",
      "name": "RIM Mapping"
    },
    {
      "identity": "w5",
      "uri": "http://hl7.org/fhir/fivews",
      "name": "FiveWs Pattern Mapping"
    },
    {
      "identity": "sct-attr",
      "uri": "http://snomed.org/attributebinding",
      "name": "SNOMED CT Attribute Binding"
    }
  ],
  "kind": "resource",
  "abstract": false,
  "type": "Observation",
  "baseDefinition": "http://hl7.org/fhir/us/core/StructureDefinition/us-core-observation-lab",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Observation.id",
        "path": "Observation.id",
        "min": 1,
        "mustSupport": true
      },
      {
        "id": "Observation.extension",
        "path": "Observation.extension",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/us/home-lab-report/StructureDefinition/Extension-at-home-in-vitro-test-proctored-info"
            ]
          }
        ]
      },
      {
        "id": "Observation.status",
        "path": "Observation.status",
        "fixedCode": "final"
      },
      {
        "id": "Observation.code",
        "path": "Observation.code",
        "short": "Consult LIVD: https://www.cdc.gov/csels/dls/livd-codes.html to obtain the values to be used for each particular use case."
      },
      {
        "id": "Observation.subject",
        "path": "Observation.subject",
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://hl7.org/fhir/us/home-lab-report/StructureDefinition/Patient-at-home-in-vitro-test"
            ]
          }
        ]
      },
      {
        "id": "Observation.effective[x]",
        "path": "Observation.effective[x]",
        "min": 1,
        "type": [
          {
            "code": "dateTime",
            "extension": [
              {
                "url": "http://hl7.org/fhir/StructureDefinition/elementdefinition-type-must-support",
                "valueBoolean": true
              }
            ]
          }
        ]
      },
      {
        "id": "Observation.issued",
        "path": "Observation.issued",
        "short": "Test result date released",
        "definition": "Test result date released: This should be populated with the test result date released, which, in the at-home setting, will mostly be the same as the Observation.effective[x], however, it could be different when a proctor is involved and releases the result. It is mapped to HL7 V2 OBR 22 because it is the required field in the message.",
        "mustSupport": true
      },
      {
        "id": "Observation.performer",
        "path": "Observation.performer",
        "short": "If the performer is not the patient being tested, specify the person performing the test.",
        "mustSupport": true
      },
      {
        "id": "Observation.value[x]",
        "path": "Observation.value[x]",
        "label": "At-Home Test Result Value",
        "short": "At-Home Test Result Value",
        "type": [
          {
            "code": "CodeableConcept",
            "extension": [
              {
                "url": "http://hl7.org/fhir/StructureDefinition/elementdefinition-type-must-support",
                "valueBoolean": true
              }
            ]
          }
        ]
      },
      {
        "id": "Observation.specimen",
        "path": "Observation.specimen",
        "short": "The Specimen SHALL be from at-home sample.",
        "definition": "The referenced specifimen should contain a Specimen.identifier and a Specimen.identifier.assigner",
        "comment": "The referenced specifimen should contain a Specimen.identifier and a Specimen.identifier.assigner",
        "min": 1,
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://hl7.org/fhir/us/home-lab-report/StructureDefinition/Specimen-at-home-in-vitro-test"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Observation.device",
        "path": "Observation.device",
        "label": "At home test device",
        "short": "The referenced device SHALL be at-home test device.",
        "definition": "The referenced device is an at-home test device, The referenced device **SHALL** contain a Device.udiCarrier.deviceIdentifier. Please see GUDID data base: https://accessgudid.nlm.nih.gov/help/search/basic-search to find the device's UDI",
        "min": 1,
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://hl7.org/fhir/us/home-lab-report/StructureDefinition/Device-at-home-in-vitro-test"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Observation.hasMember",
        "path": "Observation.hasMember",
        "short": "Multiplex Assay tests that serve as a single test to diagnose infection caused by multiple viruses (e.g. CDC Flu SC2 Multiplex Assay for SARS-CoV-2, influenza A, and/or influenza B viruses) should be recorded using multiple Observation.hasMember elements.",
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://hl7.org/fhir/us/home-lab-report/StructureDefinition/Observation-at-home-in-vitro-test-result"
            ]
          }
        ]
      }
    ]
  }
}
